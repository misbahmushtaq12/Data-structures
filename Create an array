#include <stdio.h>

int main() {
    // Declare an integer array of size 5.
    int myArray[5];

    // Initialize the array with values.
    myArray[0] = 10;
    myArray[1] = 20;
    myArray[2] = 30;
    myArray[3] = 40;
    myArray[4] = 50;

    // Access and print the elements of the array.
    printf("Elements of the array:\n");
    for (int i = 0; i < 5; i++) {
        printf("myArray[%d] = %d\n", i, myArray[i]);
    }

    return 0;
}
