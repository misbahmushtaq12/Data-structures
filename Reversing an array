#include <stdio.h>

int main() {
    int arr[5] = {10, 20, 30, 40, 50};
    int reversed[5];
    
    for (int i = 0; i < 5; i++) {
        reversed[i] = arr[4 - i];
    }

    printf("Original Array: ");
    for (int i = 0; i < 5; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");

    printf("Reversed Array: ");
    for (int i = 0; i < 5; i++) {
        printf("%d ", reversed[i]);
    }
    printf("\n");

    return 0;
}
